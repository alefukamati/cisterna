WebServer server(80);
Adafruit_SSD1306 display(SCREEN_WIDTH, SCREEN_HEIGHT, &Wire, OLED_RESET);


void setup() {
  Serial.begin(115200);

  //setar sensores
  pinMode(sensor1, INPUT);
  pinMode(sensor2, INPUT);
  //pinMode(sensor3, INPUT);

  // prepare LED
  pinMode(LED_BUILTIN, OUTPUT);
  pinMode(LED_S1, OUTPUT);
  pinMode(LED_S2, OUTPUT);
  digitalWrite(LED_BUILTIN, 0);
  digitalWrite(LED_S1, 0);
  digitalWrite(LED_S2, 0);
  WiFi.mode(WIFI_STA);
  WiFi.begin(ssid, password);

  if(!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) { 
    Serial.println(F("SSD1306 allocation failed"));
    for(;;); // Don't proceed, loop forever
  }
  display.clearDisplay();
  display.setTextSize(1);         
  display.setTextColor(WHITE); 
  while (WiFi.status() != WL_CONNECTED) {   
    display.setCursor(0, 10);       
    display.println("CONECTE-SE A UMA REDE");
    display.display();
    delay(500);
    Serial.print(F("."));
  }
  Serial.println();
  Serial.println(F("WiFi connected"));

  server.on("/", handle_OnConnect);
  server.onNotFound(handle_NotFound); 
  // Start the server
  server.begin();
  Serial.println(F("Server started"));
  Serial.println(WiFi.localIP());
  display.clearDisplay();
  display.setTextSize(1);         
  display.setTextColor(WHITE);    
  display.setCursor(0, 10);       
  display.println("Endereco: ");
  display.println(WiFi.localIP());
  display.display();
}

void loop() {
  display.clearDisplay();
  display.setCursor(0, 10);       
  display.println("Endereco: ");
  display.println(WiFi.localIP());
  if ((digitalRead(sensor1)+digitalRead(sensor2)) == 1){
   digitalWrite(LED_S1, 1);
   digitalWrite(LED_S2, 0);
   display.drawBitmap(0, 0, cisterna0, 128, 64, 1);
  }
  else if ((digitalRead(sensor1)+digitalRead(sensor2)) == 2){
   digitalWrite(LED_S1, 1);
   digitalWrite(LED_S2, 1);
   display.drawBitmap(0, 0, cisterna1, 128, 64, 1);
  }
  else{
   digitalWrite(LED_S1, 0);
   digitalWrite(LED_S2, 0);
   display.drawBitmap(0, 0, cisterna2, 128, 64, 1);
  }
  display.display();
  server.handleClient(); //Chama o método handleClient()
  //aumenta o tmepo de tolerancia até definir como timeout
}

int calc_volume(){
  int vol = 0;
  uint8_t n0 = 0;
  uint8_t n1 = 0;
  uint8_t n2 = 0;
  int maior = 0;
  int maior_n = 0;
  int s1 = 0;
  int s2 = 0;

  for(int i = 1; i<READ_TRY; i++){
    s1 = digitalRead(sensor1);
    s2 = digitalRead(sensor2);
    vol = s1+s2; 
    if(vol == 0) n0 += 1;
    if(vol == 1) n1 += 1;
    if(vol == 2) n2 += 1;
  }
  int numeros[3] = {n0, n1, n2};
  for(int i = 0; i <= 2; i++){
    if(numeros[i] > maior){
       maior = numeros[i];
       maior_n = i;
    }
  }
  Serial.print("Niveis: ");
  Serial.println(maior_n); 
  return maior_n;
}


void handle_OnConnect() {
  uint8_t s1 = 0;
  uint8_t s2 = 0;
  int vol_nivel = 0;
  digitalWrite(LED_BUILTIN, 1);
  vol_nivel = calc_volume();
  server.send(200, "text/html", EnvioHTML(vol_nivel));
 //Envia as informações usando o código 200, especifica o conteúdo como "text/html" e chama a função EnvioHTML

}

void handle_NotFound() { //Função para lidar com o erro 404
  server.send(404, "text/plain", "Não encontrado"); //Envia o código 404, especifica o conteúdo como "text/pain" e envia a mensagem "Não encontrado"

}



